[sub_resource type="GDScript" id="GDScript_hf4f0"]
resource_name = "Transition"
script/source = "extends Control

@export var path: String;

# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	$AnimationPlayer.play(\"Enter\");

func transition(target: String) -> void:
	path = target;
	$AnimationPlayer.play(\"Exit\");

func cleanup_collisions() -> void:
	#Clear all nodes first, because collision objects can't be present when changing scene
		for n: Node in get_node(\"/root/Main\").get_children():
			if n.name != \"UI\":
				n.queue_free()
		await get_tree().process_frame
		get_tree().change_scene_to_file(\"res://Scenes/Title.tscn\")

func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	# Only transisiton when it is the exit animation
	if (anim_name == \"Exit\"):
		cleanup_collisions();
		get_tree().change_scene_to_file(path);
"

