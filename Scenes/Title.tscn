[node name="AnimationPlayer" type="AnimationPlayer" parent="."]
libraries = {
&"": SubResource("AnimationLibrary_u7v65")
}

[node name="SceneTransition" parent="." instance=ExtResource("3_ud5ar")]
layout_mode = 1

[connection signal="button_down" from="mainMenu/playButton" to="." method="Play"]
[connection signal="pressed" from="mainMenu/OptionsButton" to="." method="_on_options_button_pressed"]
[connection signal="button_down" from="mainMenu/QuitButton" to="." method="Quit"]
[connection signal="timeout" from="Options/OptionsManager/Save Interval" to="Options/OptionsManager" method="_on_save_interval_timeout"]
[connection signal="value_changed" from="Options/AudioControls/MasterVolumeSlider" to="Options/OptionsManager" method="_on_master_volume_slider_value_changed"]
[connection signal="value_changed" from="Options/AudioControls/MusicVolumeSlider" to="Options/OptionsManager" method="_on_music_volume_slider_value_changed"]
[connection signal="value_changed" from="Options/AudioControls/SfxVolumeSlider" to="Options/OptionsManager" method="_on_sfx_volume_slider_value_changed"]
[connection signal="toggled" from="Options/Visualcontrols/crtButton" to="Options/OptionsManager" method="_on_crt_shader_toggled"]
